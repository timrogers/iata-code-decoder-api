{
  "name": "iata-code-decoder-api",
  "version": "1.0.0",
  "description": "A simple API, written in Node.js with the Fastify framework, which allows you to identify airports, airlines and aircraft by their IATA code",
  "type": "module",
  "scripts": {
    "generate-airports": "node scripts/generate_airports_json.js",
    "generate-airlines": "node scripts/generate_airlines_json.js",
    "generate-aircraft": "node scripts/generate_aircraft_json.js",
    "start": "node src/index.js",
    "dev": "nodemon -I --exec node --loader ts-node/esm ./src/index.ts",
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "prettier": "prettier src/**/*.ts",
    "prettier-fix": "prettier src/**/*.ts --write",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix"
  },
  "author": "Tim Rogers <me@timrogers.co.uk>",
  "license": "MIT",
  "engines": {
    "node": "24.x"
  },
  "dependencies": {
    "@fastify/compress": "^8.3.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@modelcontextprotocol/sdk": "^1.13.3",
    "dotenv": "^16.4.7",
    "fastify": "^5.6.2"
  },
  "devDependencies": {
    "@duffel/api": "^4.18.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.22.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autocannon": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.9",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "repository": "timrogers/iata-code-decoder-api"
}
